{
  "name": "servidor",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js --env prod --persistence=mongo",
    "test": "node ./server.js",
    "dev": "nodemon ./server.js --env dev",
    "argv": "nodemon ./server.js -p 8080 --env prod --persistence=fs",
    "memory": "nodemon ./server.js -p 8081 --env prod --persistence=memory",
    "fs": "nodemon ./server.js -p 8082 --env prod --persistence=fs",
    "producto": "node ./src/mocks/products.mocks.js --env dev",
    "usuario": "node ./src/mocks/users.mocks.js",
    "simplex": "artillery quick --count 20 --num 100 http://localhost:8080/simplex -o ./test/artillery/simplex.json",
    "complex": "artillery quick --count 20 --num 100 http://localhost:8080/complex -o ./test/artillery/complex.json",
    "flow": "artillery run ./test/artillery/flow.yaml -o ./test/artillery/flow.json",
    "report": "artillery report ./test/artillery/flow.json",
    "image": "docker build -t eductplataform .",
    "dockerhub1": "docker tag eductplataform santyalvarezz/educplataform:1.0.0",
    "dockerhub2": "docker push santyalvarezz/educplataform:1.0.0",
    "orq": "kubectl apply -f ./orq.yaml",
    "get-deployment": "kubectl get deployments",
    "get-services": "kubectl get services",
    "get-pods": "kubectl get pods"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "artillery-plugin-metrics-by-endpoint": "^1.20.0",
    "bcrypt": "^6.0.0",
    "commander": "^14.0.0",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-compression": "^1.0.2",
    "express-handlebars": "^8.0.3",
    "express-session": "^1.18.2",
    "joi": "^18.0.1",
    "joi-oid": "^1.1.2",
    "jsbarcode": "^3.12.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "mongoose-paginate-v2": "^1.9.1",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "nodemon": "^3.1.10"
  }
}
